        <http:binding verb="GET"/>
        <wsdl:operation name="phylotastic_GetScientificNames_GNRD_GET">
            <http:operation location="/fn/names_url"/>
            <wsdl:input>
                <http:urlEncoded/>
            </wsdl:input>
            <wsdl:output>
                <mime:content type="application/json"/>
            </wsdl:output>
        </wsdl:operation>
    </wsdl:binding>

